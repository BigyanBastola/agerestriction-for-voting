import java.util.Scanner;
public class agerestriction {
    public static void main (String args []) {
        int age;
        Scanner s = new Scanner(System.in);
        age = s.nextInt();
        if (age >= 18) {
            System.out.println("Eligible for voting");
        }else{
                System.out.print("not eligible for voting");


        }

    }}
